{
    "name": "redux-app",
    "version": "0.1.1",
    "description": "React/Redux Giphy search engine",
    "author": "Jazzy Guo",
    "license": "ISC",
    "scripts": {
        "start": "npm run server & npm run sass:watch",
        "server": "webpack-dev-server --progress --hot",
        "sass": "node-sass-chokidar src -o src --recursive --output-style compressed",
        "sass:watch": "node-sass-chokidar src -o src --recursive --output-style compressed --watch"
    },
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "css-loader": "^0.28.4",
        "style-loader": "^0.18.2",
        "webpack": "3.6.0",
        "webpack-dev-server": "2.11.1"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "babel-loader": "^7.1.4",
        "lodash": "^4.17.5",
        "node-sass": "^4.7.2",
        "node-sass-chokidar": "^1.2.0",
        "react": "^16.2.0",
        "react-dom": "^16.0.0",
        "react-redux": "^5.0.7",
        "react-thunk": "^1.0.0",
        "redux": "^3.7.2",
        "redux-thunk": "^2.2.0"
    }
}
